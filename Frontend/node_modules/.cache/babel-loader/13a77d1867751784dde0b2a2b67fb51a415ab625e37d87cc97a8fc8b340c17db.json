 method\r\n    const logout = () => {\r\n      localStorage.removeItem('isAuthenticated'); // Remove authentication state from localStorage\r\n      router.push('/'); // Redirect peoples to login page\r\n    };\r\n\r\n    return {\r\n      isActive,\r\n      isAuthenticated,\r\n      logout,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* General Reset */\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n#compName{\r\n  font-family: 'Montserrat', sans-serif;\r\n}\r\n\r\n/* ody styling */\r\nbody {\r\n  font-family: 'Arial', sans-serif;\r\n  background-color: #f4f4f9;\r\n  color: #333;\r\n  padding: 20px;\r\n}\r\n\r\n/* Header */\r\n#Header {\r\n  background-color: #1e2a3a;\r\n  color: white;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n#Header img {\r\n  max-width: 100px;\r\n  height: auto;\r\n  margin-right: 20px;\r\n}\r\n\r\n#Header h2 {\r\n  font-size: 1.8em;\r\n  font-weight: 600;\r\n  color: #fff;\r\n}\r\n\r\n#Header div {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n#Header img:last-of-type {\r\n  border-radius: 50%;\r\n  width: 40px;\r\n  height: 40px;\r\n  object-fit: cover;\r\n  margin-right: 10px;\r\n}\r\n\r\n#Header h4 {\r\n  font-size: 1.1em;\r\n  font-weight: 400;\r\n  margin-right: 20px;\r\n  color: #fff;\r\n}\r\n\r\n#Header button {\r\n  padding: 8px 16px;\r\n  background-color: #e74c3c;\r\n  border: none;\r\n  color: white;\r\n  font-weight: 600;\r\n  cursor: pointer;\r\n  border-radius: 4px;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n#Header button:hover {\r\n  background-color: #c0392b;\r\n}\r\n\r\n/* Navbar styling */\r\n.nav-tabs {\r\n  border-bottom: 2px solid #ddd;\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 10px;\r\n}\r\n\r\n.nav-item {\r\n  margin: 0 15px;\r\n}\r\n\r\n.nav-link {\r\n  color: #555;\r\n  font-size: 1.1em;\r\n  font-weight: 500;\r\n  padding: 10px 15px;\r\n  border-radius: 4px;\r\n  transition: all 0.3s ease;\r\n  text-decoration: none;\r\n}\r\n\r\n.nav-link:hover {\r\n  background-color: #ddd;\r\n}\r\n\r\n.nav-link.active {\r\n  background-color: #1e2a3a;\r\n  color: white;\r\n  border-radius: 4px;\r\n}\r\n\r\n.nav-item .nav-link.active:hover {\r\n  background-color: #34495e;\r\n}\r\n\r\n/* main content area */\r\n#app {\r\n  background-color: white;\r\n  border-radius: 8px;\r\n  box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.1);\r\n  padding: 20px;\r\n}\r\n\r\n\r\n\r\n</style>\r\n\r\n\r\n<!-- color: #42b983; -->\r\n\r\n<!-- Styles for media responsiveness -->\r\n<style>\r\n/* Default Styles (Desktop-first approach) */\r\nbody {\r\n  font-size: 16px;\r\n  margin: 0;\r\n  padding: 0;\r\n  font-family: Arial, sans-serif;\r\n}\r\n\r\n.container {\r\n  width: 80%;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n/* Laptop (1024px) */\r\n@media (max-width: 1024px) {\r\n  .container {\r\n    width: 90%;\r\n    padding: 15px;\r\n  }\r\n\r\n  body {\r\n    font-size: 15px;\r\n  }\r\n}\r\n\r\n/* Tablet (768px) */\r\n@media (max-width: 768px) {\r\n  .container {\r\n    width: 95%;\r\n    padding: 10px;\r\n  }\r\n\r\n  body {\r\n    font-size: 14px;\r\n  }\r\n\r\n  .header {\r\n    font-size: 18px;\r\n    text-align: center;\r\n  }\r\n}\r\n\r\n/* Mobile L (425px) */\r\n@media (max-width: 425px) {\r\n  .container {\r\n    width: 100%;\r\n    padding: 5px;\r\n  }\r\n\r\n  body {\r\n    font-size: 13px;\r\n  }\r\n\r\n  .header {\r\n    font-size: 16px;\r\n  }\r\n\r\n  .button {\r\n    width: 100%;\r\n    font-size: 14px;\r\n    padding: 10px;\r\n  }\r\n}\r\n\r\n/* Mobile M (375px) */\r\n@media (max-width: 375px) {\r\n  body {\r\n    font-size: 12px;\r\n  }\r\n\r\n  .header {\r\n    font-size: 14px;\r\n  }\r\n\r\n  .button {\r\n    font-size: 13px;\r\n    padding: 8px;\r\n  }\r\n}\r\n\r\n/* Mobile S (320px) */\r\n@media (max-width: 320px) {\r\n  body {\r\n    font-size: 11px;\r\n  }\r\n\r\n  .header {\r\n    font-size: 12px;\r\n  }\r\n\r\n  .button {\r\n    font-size: 12px;\r\n    padding: 6px;\r\n  }\r\n}\r\n\r\n</style>"],"mappings":";OAKWA,UAAkC;OAIlCC,UAAyB;;EAN7BC,EAAE,EAAC;AAAQ;;EAcXA,EAAE,EAAC;AAAK;;EACPC,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAAU;;EAKhBA,KAAK,EAAC;AAAU;;EAKhBA,KAAK,EAAC;AAAU;;EAKhBA,KAAK,EAAC;AAAU;;;;uBAlC1BC,mBAAA,CAAAC,SAAA,SAGEC,mBAAA,CAUM,OAVNC,UAUM,G,0BATJD,mBAAA,CAGM,cAFJA,mBAAA,CAAuE;IAAlEE,GAAkC,EAAlCR,UAAkC;IAACS,GAAG,EAAC;MAC5CH,mBAAA,CAAiC;IAA7BJ,EAAE,EAAC;EAAU,GAAC,YAAU,E,sBAE9BI,mBAAA,CAIM,c,0BAHJA,mBAAA,CAAsD;IAAjDE,GAAyB,EAAzBP,UAAyB;IAACQ,GAAG,EAAC;yDACnCH,mBAAA,CAAoB,YAAhB,aAAW,sBACeI,MAAA,CAAAC,eAAe,M,cAA7CP,mBAAA,CAAgE;IAXtEQ,GAAA;IAWeC,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEL,MAAA,CAAAM,MAAA,IAAAN,MAAA,CAAAM,MAAA,IAAAD,IAAA,CAAM;KAA2B,QAAM,KAX7DE,mBAAA,e,KAiBEX,mBAAA,CA0BM,OA1BNY,UA0BM,GAzBJZ,mBAAA,CAqBK,MArBLa,UAqBK,GApBHb,mBAAA,CAIK,MAJLc,UAIK,GAHHC,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC,OAAO;IAACpB,KAAK,EApBrCqB,eAAA,EAoBsC,UAAU;MAAAC,MAAA,EAAmBf,MAAA,CAAAgB,QAAQ;IAAA;;IApB3EC,OAAA,EAAAC,QAAA,CAoBwF,MAEhFd,MAAA,QAAAA,MAAA,OAtBRe,gBAAA,CAoBwF,QAEhF,E;IAtBRC,CAAA;kCAwBMxB,mBAAA,CAIK,MAJLyB,UAIK,GAHHV,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC,yBAAyB;IAACpB,KAAK,EAzBvDqB,eAAA,EAyBwD,UAAU;MAAAC,MAAA,EAAmBf,MAAA,CAAAgB,QAAQ;IAAA;;IAzB7FC,OAAA,EAAAC,QAAA,CAyB4H,MAEpHd,MAAA,QAAAA,MAAA,OA3BRe,gBAAA,CAyB4H,4BAEpH,E;IA3BRC,CAAA;kCA6BMxB,mBAAA,CAIK,MAJL0B,UAIK,GAHHX,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC,cAAc;IAACpB,KAAK,EA9B5CqB,eAAA,EA8B6C,UAAU;MAAAC,MAAA,EAAmBf,MAAA,CAAAgB,QAAQ;IAAA;;IA9BlFC,OAAA,EAAAC,QAAA,CA8BsG,MAE9Fd,MAAA,QAAAA,MAAA,OAhCRe,gBAAA,CA8BsG,kCAE9F,E;IAhCRC,CAAA;kCAkCMxB,mBAAA,CAIK,MAJL2B,UAIK,GAHHZ,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC,UAAU;IAACpB,KAAK,EAnCxCqB,eAAA,EAmCyC,UAAU;MAAAC,MAAA,EAAmBf,MAAA,CAAAgB,QAAQ;IAAA;;IAnC9EC,OAAA,EAAAC,QAAA,CAmC8F,MAEtFd,MAAA,QAAAA,MAAA,OArCRe,gBAAA,CAmC8F,WAEtF,E;IArCRC,CAAA;oCA0CIT,YAAA,CAAea,sBAAA,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}